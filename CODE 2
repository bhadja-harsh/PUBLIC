n = int(input("Enter the number of tuples: "))

tuple_list = []

for i in range(n):
    elements = input(f"Enter the elements for tuple {i+1} (separated by space): ").split()
    
    if not elements:
        tuple_list.append(())
    else:
        tuple_list.append(tuple(elements))

tuple_list = [tup for tup in tuple_list if tup]

print("List after removing empty tuples:", tuple_list)
